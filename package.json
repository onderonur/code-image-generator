{
  "name": "code-image-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codequality": "npm run format && npm run lint && npm run typecheck",
    "codequality:fix": "npm run format:fix && npm run lint:fix && npm run typecheck",
    "dev": "next dev",
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix --max-warnings 0",
    "prepare": "husky",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@uiw/codemirror-extensions-langs": "4.19.8",
    "@uiw/codemirror-themes-all": "4.19.8",
    "@uiw/react-codemirror": "4.19.8",
    "@zag-js/focus-visible": "^0.40.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "next": "^14.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.5",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
