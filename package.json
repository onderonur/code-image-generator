    "classnames": "^2.3.2",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "next": "^14.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-select": "^5.7.7",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.6",
