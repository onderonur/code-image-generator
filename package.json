{
  "name": "code-image-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codequality": "npm run format && npm run lint",
    "codequality:fix": "npm run format:fix && npm run lint:fix",
    "dev": "next dev",
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix --max-warnings 0",
    "prepare": "husky install",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@uiw/codemirror-extensions-langs": "^4.19.8",
    "@uiw/codemirror-themes-all": "^4.19.8",
    "@uiw/react-codemirror": "^4.19.8",
    "@zag-js/focus-visible": "^0.32.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "next": "^14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.3",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
